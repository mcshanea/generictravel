<% fields_for_rate(rate) do |f| %>
<div class="rate">
<p>
  <%= f.label :amount %><br />
  <%= f.text_field :amount %>
</p>

<p>
  <%= f.label :start_date %><br />
	<%= f.calendar_date_select :start_date, :time => false %>
</p>

<p>
  <%= f.label :end_date %><br />
	<%= f.calendar_date_select :end_date, :time => false %>
</p>
</div>
<% end %>